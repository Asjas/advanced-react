{
  "engines": {
    "node": ">15.0.0"
  },
  "scripts": {
    "start": "keystone-next start",
    "deploy": "keystone-next build && keystone-next prisma migrate deploy",
    "predeploy": "keystone-next postinstall --fix",
    "dev": "keystone-next dev",
    "seed-data": "keystone-next --seed-data"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 14,
            "browsers": [
              "last 2 chrome versions",
              "last 2 firefox versions",
              "last 2 safari versions",
              "last 2 edge versions"
            ]
          }
        }
      ],
      "@babel/preset-react",
      "@babel/preset-typescript"
    ]
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "7.14.8",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.14.5",
    "@babel/runtime": "7.14.8",
    "@keystone-next/admin-ui": "14.1.3",
    "@keystone-next/auth": "30.0.0",
    "@keystone-next/cloudinary": "6.0.5",
    "@keystone-next/fields": "13.0.0",
    "@keystone-next/keystone": "23.0.1",
    "@keystone-next/types": "23.0.0",
    "@keystonejs/server-side-graphql-client": "2.1.2",
    "dotenv": "10.0.0",
    "next": "11.0.1",
    "nodemailer": "6.6.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "stripe": "8.165.0"
  },
  "devDependencies": {
    "@types/node": "15.3.0",
    "@types/nodemailer": "6.4.4",
    "eslint-config-asjas": "0.0.25",
    "typescript": "4.3.5"
  }
}
